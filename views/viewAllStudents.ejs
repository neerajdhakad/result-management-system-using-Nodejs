<!-- include header -->
<%-include('partials/_header') %>
<!-- /include header -->

<!-- include main -->
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex justify-between">
      <a href="/add-student" class="border-shadow">
        <span class="text-gradient"
          >Add Student <i class="fas fa-user"></i
        ></span>
      </a>
      <a href="/" class="border-shadow">
        <span class="text-gradient"
          >Logout <i class="fa fa-sign-out" aria-hidden="true"></i></i
        ></span>
      </a>
    </div>
    <!-- form handling -->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>Roll No</th>
            <th>Name</th>
            <th>DOB</th>
            <th>Score</th>
            <th>Actions</th>
          </tr>
        </thead>
        <tbody>
            <%-include('partials/_show') %>
        </tbody>
      </table>
    </form>
  </div>
</main>
<!-- /include main -->

<!-- include footer -->
<%-include('partials/_footer') %>
<!-- /include footer -->
